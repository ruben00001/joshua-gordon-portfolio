---
import type { PageRoute } from "~types/index";

const links: PageRoute[] = [
  "Home",
  "Other",
  {
    label: "Diary Part 1",
    route: "diary-part-1",
  },
];
---

{
  links.map((link, i) =>
    i === 0 ? (
      <a href={"/"}>{link}</a>
    ) : typeof link === "string" ? (
      <a href={"/" + link}>{link}</a>
    ) : (
      <a href={"/" + link.route}>{link.label}</a>
    )
  )
}
